{
	"schemaVersion": 1,
	"id": "${mod_id}",
	"version": "${version}",
	"name": "${mod_name}",
	"description": "Kotreen.",
	"authors": [
		"Huanlan233"
	],
	"contact": {
		"homepage": "https://fabricmc.net/",
		"sources": "https://github.com/FabricMC/fabric-example-mod"
	},
	"license": "AGPL-3.0",
	"icon": "assets/kotreen/icon.png",
	"environment": "*",
	"entrypoints": {
		"main": [
			{
				"value": "top.htext.kotreen.Kotreen",
				"adapter": "kotlin"
			}
		]
	},
	"mixins": [
		"kotreen.mixins.json"
	],
	"depends": {
		"fabricloader": ">={loader_version}",
		"minecraft": "${minecraft_version}",
		"fabric-api": "*",
		"fabric-language-kotlin": "*",
		"carpet": "*"
	},
	"suggests": {
		"another-mod": "*"
	}
}